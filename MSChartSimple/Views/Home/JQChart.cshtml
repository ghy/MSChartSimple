@{
    Layout = null;
}
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>HTML5 Chart jQuery Plugin - Line Chart on a Category Axis </title>
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.jqChart.min.js")" type="text/javascript"></script>
    <!--[if IE]><script lang="javascript" type="text/javascript" src="../../Scripts/excanvas.js"></script><![endif]-->
    <script language="javascript" type="text/javascript">
        $(document).ready(function () {
            $('#jqChart').jqChart({
                title: { text: 'Line Chart' },
                axes: [
                        {
                            type: 'category',
                            location: 'bottom',
                            categories: ['2:00', '4:00', '6:00', '8:00', '10:00', '12:00', '14:00']
                        }
                      ],
                series: [
                            {
                                type: 'splineArea',
                                title: 'In',
                                data: [62, 70, 68, 58, 52, 60, 48]
                            },
                            {
                                type: 'line',
                                title: 'Out',
                                data: [56, 30, 62, 65, 40, 36, 70]
                            }
                        ]
            });
        });
    </script>
</head>
<body>
    <div>
        <div id="jqChart" style="width: 600px; height: 300px;" />
    </div>
</body>
</html>
